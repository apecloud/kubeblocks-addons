{
  "$schema": "http://json-schema.org/schema#",
  "type": "object",
  "properties": {
    "disableExporter": {
      "title": "Monitor Enabled",
      "description": "Enable or disable monitor.",
      "type": "boolean",
      "default": true
    },
    "terminationPolicy": {
      "title": "Termination Policy",
      "description": "The termination policy of cluster.",
      "type": "string",
      "default": "Delete",
      "enum": [
        "DoNotTerminate",
        "Delete",
        "WipeOut"
      ]
    }
  }
}
