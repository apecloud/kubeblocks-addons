{
  "$schema": "http://json-schema.org/schema#",
  "type": "object",
  "properties": {
    "nameOverride": {
      "title": "Name",
      "description": "The override name.",
      "type": "string",
      "default": ""
    },
    "fullnameOverride": {
      "title": "Fullname",
      "description": "The override fullname.",
      "type": "string",
      "default": ""
    },
    "terminationPolicy": {
      "title": "TerminationPolicy",
      "description": "The policy of termination.",
      "type": "string",
      "default": "Delete",
      "enum": [
        "Delete",
        "DoNotTerminate",
        "Halt",
        "WipeOut"
      ]
    },
    "vminsert": {
      "type": "object",
      "properties": {
        "replicas": {
          "title": "Replicas for vminsert",
          "description": "The number of replicas for vminsert component.",
          "type": [
            "number",
            "string"
          ],
          "default": 1,
          "minimum": 1,
          "maximum": 5
        },
        "resources": {
          "type": "object",
          "properties": {
            "limits": {
              "type": "object",
              "properties": {
                "memory": {
                  "title": "Memory(Gi) for vminsert",
                  "description": "Memory limits for vminsert component, the unit is Gi.",
                  "type": [
                    "number",
                    "string"
                  ],
                  "default": 0.5,
                  "minimum": 0.5,
                  "maximum": 1000
                },
                "cpu": {
                  "title": "CPU for vminsert",
                  "description": "CPU cores limits for vminsert component.",
                  "type": [
                    "number",
                    "string"
                  ],
                  "default": 0.5,
                  "minimum": 0.5,
                  "maximum": 64,
                  "multipleOf": 0.5
                }
              }
            },
            "requests": {
              "type": "object",
              "properties": {
                "memory": {
                  "title": "Memory(Gi) for vminsert",
