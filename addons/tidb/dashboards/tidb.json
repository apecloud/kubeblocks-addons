      "pluginVersion": "10.1.1",
      "targets": [
        {
          "datasource": {
            "uid": "$datasource"
          },
          "expr": "histogram_quantile(0.99, sum(rate(tidb_server_handle_query_duration_seconds_bucket{namespace=\"$namespace\", app_kubernetes_io_instance=\"$cluster\", pod=~\"$instance\", sql_type!=\"internal\"}[1m])) by (le))",
          "format": "time_series",
          "intervalFactor": 2,
          "legendFormat": "99",
          "refId": "A"
        },
        {
          "datasource": {
            "uid": "$datasource"
          },
          "expr": "histogram_quantile(0.95, sum(rate(tidb_server_handle_query_duration_seconds_bucket{namespace=\"$namespace\", app_kubernetes_io_instance=\"$cluster\", pod=~\"$instance\", sql_type!=\"internal\"}[1m])) by (le))",
          "format": "time_series",
          "intervalFactor": 2,
          "legendFormat": "95",
          "refId": "B"
        },
        {
          "datasource": {
            "uid": "$datasource"
          },
          "expr": "sum(rate(tidb_server_handle_query_duration_seconds_sum{namespace=\"$namespace\", app_kubernetes_io_instance=\"$cluster\", pod=~\"$instance\", sql_type!=\"internal\"}[30s])) / sum(rate(tidb_server_handle_query_duration_seconds_count{namespace=\"$namespace\", app_kubernetes_io_instance=\"$cluster\", sql_type!=\"internal\"}[30s]))",
          "format": "time_series",
          "intervalFactor": 2,
          "legendFormat": "avg",
          "refId": "C"
        }
      ],
      "title": "Duration",
      "type": "timeseries"
